{
    "name": "nest-project",
    "version": "1",
    "description": "nest-project",
    "author": "",
    "license": "MIT",
    "scripts": {
        "build:prod": "tsc -p tsconfig.build.json",
        "postbuild:prod": "copyfiles --up 1 src/**/*.json dist",
        "start:hmr": "node dist/main.hmr.ts",
        "start:dev": "ts-node src/main.ts",
        "start:prod": "node dist/src/main.js",
        "run:docker-services": "docker compose -f external-services.yml up",
        "typeorm": "ts-node node_modules/typeorm/cli.js  -f ormconfig",
        "migration:generate": "yarn run typeorm migration:generate -d src/migrations -n",
        "migration:create": "yarn run typeorm migration:create -d src/migrations -n",
        "migration:revert": "yarn run typeorm migration:revert",
        "migration:run": "yarn run typeorm migration:run",
        "schema:drop": "yarn run typeorm schema:drop",
        "watch:dev": "nodemon --config nodemon.json",
        "debug:dev": "nodemon --config nodemon-debug.json",
        "webpack": "webpack --config webpack.config.js --progress",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts",
        "test": "jest --config ./jest.json",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand"
    },
    "dependencies": {
        "@golevelup/nestjs-testing": "^0.1.2",
        "@nestjs/axios": "^0.0.5",
        "@nestjs/common": "^8.2.3",
        "@nestjs/config": "^1.1.6",
        "@nestjs/core": "^8.2.3",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/microservices": "^8.2.3",
        "@nestjs/passport": "^8.1.0",
        "@nestjs/platform-express": "^8.2.3",
        "@nestjs/swagger": "^5.1.5",
        "@nestjs/typeorm": "^8.0.2",
        "@types/bcrypt": "^5.0.0",
        "@types/lodash": "^4.14.168",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "copyfiles": "^2.4.1",
        "decimal.js-light": "^2.5.1",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.2.6",
        "helmet": "^4.5.0",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.6.0",
        "reflect-metadata": "^0.1.13",
        "request-context": "^2.0.0",
        "rxjs": "^7",
        "source-map-support": "^0.5.19",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.32",
        "typeorm-transactional-cls-hooked": "^0.1.20",
        "typescript": "^4.2.4",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/testing": "^8.2.3",
        "@types/compression": "^1.7.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.11",
        "@types/express-rate-limit": "^5.1.1",
        "@types/helmet": "^4.0.0",
        "@types/jest": "^26.0.22",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.41",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-import-helpers": "^1.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "gh-pages": "^3.1.0",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "lint-staged": "~10.5.4",
        "nodemon": "^2.0.7",
        "prettier": "^2.0.5",
        "supertest": "^6.2.2",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.1.0",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-consistent-codestyle": "^1.16.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-plugin-prettier": "^2.3.0",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "git add"
        ]
    }
}
